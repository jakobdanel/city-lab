    <h1>Plants</h1>
    <form action="/plantManager/create" method="post" role="form">
      <div class="input-row">
        <label for="plantName">Plant</label>
        <input type="text" name="plantName" id="plantName" required/>
      </div>
      <div class="input-row">
        <label for="plantSpecies">Species</label>
        <input type="text" name="plantSpecies" id="plantSpecies" required/>
      </div>
      <div class="input-row">
        <label for="details">Details</label>
        <input type="description" name="details" id="details" required/>
      </div>
      <div class="input-row">
        <label for="imgUrl">Image Url</label>
        <input type="Text" name="imgUrl" id="imgUrl" />
      </div>
      <div class="input-row">
        <label for="dataUrl">Data Url</label>
        <input type="Text" name="dataUrl" id="dataUrl" />
      </div>
     
      <button type="submit" name="action">Submit</button>
    </form>
